{
  "name": "uccellolabs/filament-tenancy",
  "description": "Easy way to manage tenancy with Filament.",
  "type": "library",
  "require": {
    "php": "^8.0",
    "filament/filament": "^2.0",
    "spatie/laravel-sluggable": "^3.4"
  },
  "license": "MIT",
  "autoload": {
    "psr-4": {
      "Uccellolabs\\FilamentTenancy\\": "src/"
    }
  },
  "authors": [
    {
      "name": "Jonathan SARDO",
      "email": "jonathan@uccellolabs.com"
    }
  ],
  "minimum-stability": "dev",
  "config": {
    "sort-packages": true
  },
  "extra": {
    "laravel": {
      "providers": [
        "Uccellolabs\\FilamentTenancy\\FilamentTenancyProvider"
      ]
    }
  },
  "prefer-stable": true
}
